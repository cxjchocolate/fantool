<table>
    <thead>
        <tr>
            <th>任务名称</th>
            <th>任务明细</th>
            <th>更新日期</th>
            <th>状态</th>
        </tr>
    </thead>
    <tbody>
    {% for obj in objs %}
        <tr>
            <td>{{obj.name}}</td>
            <td class="long">{{obj.data}}</td>
            <td>{{obj.createtime}}</td>
            <td>-</td>
        </tr>
    {% end %}
    </tbody>
</table>